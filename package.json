{
    "name": "smart-twitch-directory",
    "version": "0.1.2",
    "description": "Find the content you want to watch on twitch!",
    "dependencies": {
        "@tensorflow/tfjs": "^4.2.0",
        "assert": "^2.0.0",
        "axios": "^1.3.4",
        "buffer": "^6.0.3",
        "crypto-browserify": "^3.12.0",
        "decision-tree": "^0.3.7",
        "lodash": "^4.17.21",
        "ml-random-forest": "^2.1.0",
        "moment": "^2.29.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-loader-spinner": "^5.3.4",
        "stream-browserify": "^3.0.0",
        "styled-components": "5.3.0",
        "webextension-polyfill": "^0.10.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@types/axios": "^0.14.0",
        "@types/lodash": "^4.14.191",
        "@types/underscore": "^1.11.4",
        "@types/webextension-polyfill": "^0.10.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "babel-loader": "^9.1.2",
        "copy-webpack-plugin": "^11.0.0",
        "dotenv": "^16.0.3",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.36.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-react": "^7.32.2",
        "source-map-loader": "^4.0.1",
        "transform-json-webpack-plugin": "^0.0.2",
        "ts-loader": "^9.4.2",
        "typescript": "^5.0.2",
        "webpack": "^5.76.2",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.13.1",
        "worker-loader": "^3.0.8",
        "worker-plugin": "^5.0.1"
    },
    "resolutions": {
        "react-loader-spinner/styled-components": "5.3.0"
    },
    "scripts": {
        "watch": "webpack --watch --progress --config webpack.config.js",
        "start": "web-ext run --source-dir dist --firefox deved",
        "start-chrome": "web-ext run --source-dir dist --target chromium",
        "build": "webpack --mode production && web-ext build --source-dir dist --overwrite-dest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "server-start": "webpack-dev-server --mode development --open",
        "build-worker": "webpack --config webpack.worker.config.js"
    }
}
