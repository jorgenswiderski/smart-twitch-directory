{
    "name": "twitch-helper",
    "version": "0.0.1",
    "description": "Find the content you want to watch on twitch!",
    "dependencies": {
        "axios": "^1.3.4",
        "moment": "^2.29.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-loader-spinner": "^5.3.4",
        "styled-components": "5.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@types/axios": "^0.14.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "babel-loader": "^9.1.2",
        "copy-webpack-plugin": "^11.0.0",
        "dotenv": "^16.0.3",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.36.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-react": "^7.32.2",
        "source-map-loader": "^4.0.1",
        "transform-json-webpack-plugin": "^0.0.2",
        "ts-loader": "^9.4.2",
        "typescript": "^5.0.2",
        "webpack": "^5.76.2",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.13.1"
    },
    "resolutions": {
        "react-loader-spinner/styled-components": "5.3.0"
    },
    "scripts": {
        "watch": "webpack --watch --progress --config webpack.config.js",
        "start": "web-ext run --source-dir dist --firefox deved",
        "start-chrome": "web-ext run --source-dir dist --target chromium",
        "build": "webpack --mode production",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "server-start": "webpack-dev-server --mode development --open"
    }
}
